{
  "manifest_version": 2,

  "name": "My Bookmarks",
  "description": "This will helps you to see my favourite sites",
  "version": "1.0",

  "browser_action": {
    "default_icon": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}